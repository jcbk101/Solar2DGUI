//---------------------------------------------------------------------------

#pragma hdrstop

#include "TRectangleEx.h"

#pragma package(smart_init)


void __fastcall TRectangleEx::TRectangleEx(TComponent* Owner)
{
	this->animations = new TList();
}


__fastcall TRectangleEx::~TRectangleEx()
{
	delete this->animations;
}
